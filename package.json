{
  "name": "chef",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ionic serve --no-open",
    "serve:local": "env-cmd -f ./.env.local vue-cli-service serve",
    "serve:prod": "env-cmd -f ./.env.prod vue-cli-service serve",
    "build": "env-cmd -f ./.env.prod vue-cli-service build",
    "add:android": "npx cap add android",
    "sync:android": "npx cap sync android",
    "open:android": "npx cap open android",
    "run:android": "npx cap run android",
    "test": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "prepare": "husky install",
    "lint": "npm run eslint",
    "eslint": "eslint . --ext .ts,.vue --fix"
  },
  "dependencies": {
    "@capacitor/android": "^2.5.0",
    "@capacitor/core": "^2.5.0",
    "@ionic/vue": "^5.9.2",
    "@ionic/vue-router": "^5.9.2",
    "core-js": "^3.19.3",
    "dotenv": "^8.6.0",
    "env-cmd": "^10.1.0",
    "typescript": "^4.5.3",
    "vue": "^3.2.24",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@capacitor/cli": "2.4.7",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^1.0.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-e2e-cypress": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-router": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "4.5.11",
    "@vue/compiler-sfc": "^3.2.24",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-rc.17",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.20.0",
    "husky": "^5.2.0",
    "prettier": "2.2.1",
    "sass": "^1.44.0",
    "sass-loader": "^10.2.0",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "description": "An Ionic project"
}
